<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>name</key>
	<string>Mail</string>
	<key>optional</key>
	<dict>
		<key>subject</key>
		<string>&amp;subject={{subject}}</string>
		<key>messageBody</key>
		<string>&amp;body={{messageBody}}</string>
	</dict>
	<key>actions</key>
	<dict>
		<key>sendMailTo:</key>
		<string>mailto:{{recipient}}</string>
	</dict>
</dict>
</plist>
